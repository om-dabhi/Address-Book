@model Country_State_City_Final.Areas.Branch.Models.Branchmodel

<form method="post" asp-action="BranchSave">

     @Html.HiddenFor(x=>x.BranchId)
     <div class="mb-3">
        <label class="form-label">Branch Name</label>
        <input type="text" class="form-control" asp-for="BranchName">
        <span class="form-text" asp-validation-for="BranchName"> </span>
    </div>

    <div class="mb-3">
        <label class="form-label">Branch Code</label>
        <input type="text" class="form-control" asp-for="BranchCode">
        <span class="form-text" asp-validation-for="BranchCode"> </span>
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>